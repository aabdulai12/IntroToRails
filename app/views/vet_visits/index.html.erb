<h1>All Vet Visits</h1>

<table class="table">
 <thead>
  <tr>
    <th>Date</th>
    <th>Description</th>
    <th>Dog Name</th>
    <th>Breed</th>
 
  </tr>
</thead>
<tbody>
  <% @vet_visits.each do |visit| %>
    <tr>
      <td><%= visit.visit_date %></td>
      <td><%= visit.description %></td>
      <td><%= visit.dog.name %></td>
      <td><%= visit.dog.breed.name %></td>
   
    </tr>
  <% end %>
</tbody>

</table>

<div class="d-flex justify-content-center">
  <%= paginate @vet_visits %>
</div>
